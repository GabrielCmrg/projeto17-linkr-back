{
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "no-unused-vars": "warn",
    "import/prefer-default-export": "off",
    "no-console": "off",
    "import/extensions": ["error", "ignorePackages"],
    "import/no-cycle": "off"
  }
}
